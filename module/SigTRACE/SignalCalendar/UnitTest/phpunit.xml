<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="Bootstrap.php" colors="true" stderr="true">
    <testsuites>
        <testsuite name="SigTRACE">
            <directory>./ArchiveTest</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>    
            <directory suffix=".php">../src/SignalCalendar/Model</directory>
            <directory suffix=".php">../src/SignalCalendar/Controller</directory>
        </whitelist>
        
    </filter>
    
    <logging>
        <log type="coverage-html" target="../../../../../test/unittest/code_coverage/SignalCalendar" lowUpperBound="40" highLowerBound="80" />
    </logging>
</phpunit>
